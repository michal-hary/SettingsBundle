{
    "name": "michal-hary/settings-bundle",
    "description": "Database centric Symfony configuration management. Global and per-user settings supported.",
    "keywords": ["symfony", "bundle", "config", "configuration", "settings"],
    "homepage": "https://github.com/michal-hary/SettingsBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Harężlak",
            "homepage": "https://github.com/michal-hary"
        },
        {
            "name": "Dmitriy Scherbina",
            "homepage": "http://dmishh.com"
        },
        {
            "name": "Tobias Nyholm",
            "homepage": "https://github.com/Nyholm"
        },
        {
            "name": "Richard van Laak",
            "homepage": "https://github.com/rvanlaak"
        }
    ],
    "require": {
        "php": "^5.5.9|^7.0",
        "psr/cache": "^1.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/form": "^3.0",
        "symfony/translation": "^3.0",
        "symfony/validator": "^3.0",
        "doctrine/orm": "^2.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "mockery/mockery": "^0.9",
        "doctrine/doctrine-bundle": "~1.2",
        "polishsymfonycommunity/symfony-mocker-container": "^1.0",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "Mhary\\SettingsBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "suggest": {
        "cache/adapter-bundle": "This bundle will help you to add your PSR-6 cache implementations as Symfony services"
    }
}
